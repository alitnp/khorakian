{
  "name": "@my/panel",
  "private": true,
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "start": "env-cmd -f .env.development react-scripts start",
    "start-production": "env-cmd -f .env.production react-scripts start",
    "start-local": "env-cmd -f .env.local react-scripts start",
    "build": "npm-run-all --parallel build:*",
