{
	"name": "workspace",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"private": true,
	"workspaces": {
		"packages": [
			"apps/*",
			"shared/*"
		]
	},
	"devDependencies": {
		"concurrently": "^7.6.0"
	},
	"installConfig": {
		"hoistingLimits": "workspaces"
	},
	"scripts": {
		"start": "concurrently \"yarn workspace @my/api start\" \"yarn workspace @my/panel start\" \"yarn workspace @my/web service:start\"",
		"start:back-web": "concurrently \"yarn workspace @my/api start\" \"yarn workspace @my/web start\"",
		"start:back-panel": "concurrently \"yarn workspace @my/api start\" \"yarn workspace @my/panel start\"",
		"start:back": "yarn workspace @my/api start",
		"start:web": "yarn workspace @my/web start",
		"web:build": "yarn workspace @my/web build",
		"web:start": "yarn workspace @my/web service:start",
		"start:panel": "yarn workspace @my/panel start"
	},
	"packageManager": "yarn@3.6.1"
}
